package demo;

import java.util.LinkedList;

public class BasicArrayList {
	public static void main(String[] args) {
//		LinkedList al = new LinkedList();
		ArrayList al = new ArrayList();
		al.add(10);
		al.add(1000);
		al.add(20);
		al.add(30);
		al.add(40);
		al.add(50);
		al.add("Pal");
		al.add('A');
		al.add(true);
		
		System.out.println(al);
	}

}
